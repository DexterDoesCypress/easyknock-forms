{
    "name": "practice-hello-world",
    "version": "1.0.0",
    "main": "index.js",
    "license": "ISC",
    "scripts": {
        "start": "node ./dist/server.js",
        "test": "jest --runInBand --silent",
        "jest-clean": "jest --clearCache",
        "prebuild": "yarn run clean && yarn build-messages",
        "build": "npm-run-all -p build-rele build-dev",
        "build-rele": "cross-env NODE_ENV=production yarn build-all",
        "build-dev": "cross-env NODE_ENV=development yarn build-all",
        "build-all": "npm-run-all -p  build-client build-server",
        "build-client": "webpack --progress",
        "build-server": "webpack --config webpack.server.config.js",
        "watch:client": "webpack --watch",
        "watch:server": "webpack --config webpack.server.config.js --watch --progress",
        "watch": "run-p watch:client watch:server",
        "type-check": "tsc --noemit",
        "build-messages": "",
        "gen:new-messages": "generate-translations && yarn build-messages && yarn fmt:messages",
        "fmt:messages": "yarn prettier --write ./src/i18n",
        "clean": "rimraf ./dist",
        "open-cypress": "yarn run cypress open",
        "cy": "yarn run cypress open",
        "cya": "yarn && yarn build && yarn run cypress open",
        "gql-gen": "node ./node_modules/@costar/mf-gql-support/scripts/gql.js generate:helpers react"
    },
    "dependencies": {
        "@elastic/elasticsearch": "7.13.0",
        "@popperjs/core": "2.11.5",
        "@radix-ui/react-dropdown-menu": "2.0.1",
        "apollo-client": "2.6.10",
        "apollo-server": "3.4.1",
        "app-root-path": "3.0.0",
        "body-parser": "^1.19.0",
        "classnames": "2.3.1",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.4",
        "cors": "^2.8.5",
        "express": "4.18.1",
        "express-graphql": "0.9.0",
        "formik": "^2.2.6",
        "fs": "0.0.2",
        "full-icu": "1.3.1",
        "handlebars": "4.7.7",
        "immutability-helper": "3.1.1",
        "jaeger-client": "3.18.0",
        "jsonwebtoken": "^8.5.1",
        "kind-of": "6.0.3",
        "less": "^3.10.3",
        "lodash": "4.17.21",
        "multer": "1.4.5-lts.1",
        "node-cache": "5.1.2",
        "os-browserify": "0.3.0",
        "path": "0.12.7",
        "path-browserify": "1.0.1",
        "query-string": "7.0.1",
        "react-autosize-textarea": "7.0.0",
        "react-debounce-input": "3.2.2",
        "react-dnd": "16.0.1",
        "react-dnd-html5-backend": "16.0.1",
        "react-dropzone": "11.0.1",
        "react-popper": "2.2.5",
        "react-router": "5.1.2",
        "react-router-dom": "5.1.2",
        "react-select-event": "4.1.4",
        "react-tooltip": "4.2.21",
        "react-window": "1.8.6",
        "regenerator-runtime": "^0.13.3",
        "request": "2.88.2",
        "superagent": "5.2.2",
        "uri-templates": "^0.2.0",
        "use-onclickoutside": "0.4.1",
        "use-query-params": "1.2.3",
        "uuid": "8.3.2",
        "xss": "1.0.10",
        "yup": "0.30.0"
    },
    "devDependencies": {
        "@cypress/webpack-preprocessor": "~5.9.0",
        "@types/cypress-cucumber-preprocessor": "~4.0.1",
        "@types/jest": "^26.0.23",
        "@types/lodash": "4.14.191",
        "@types/react": "16.9.31",
        "@types/sinon": "9.0.0",
        "cypress": "9.7.0",
        "cypress-cucumber-preprocessor": "~4.3.1",
        "react": "^17.0.1",
        "react-dom": "^17.0.2",
        "sinon": "9.0.2",
        "ts-loader": "8.3.0",
        "typescript": "3.9.2",
        "webpack": "^5.75.0"
    },
    "cypress-cucumber-preprocessor": {
        "nonGlobalStepDefinitions": true
    }
}
